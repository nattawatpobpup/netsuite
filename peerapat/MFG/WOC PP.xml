  <td class='xl643965'></td>
  <td class='xl643965'></td>
  <td class='xl643965'></td>
  <td class='xl653965'></td>
  <td colspan='2' class='xl653965'>${today?string["dd/MM/yyyy"]} ${today?string["hh:mm"]}</td>
 </tr>
 </table>
	</macro>
