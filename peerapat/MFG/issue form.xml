<body header="nlheader" header-height="27%" footer="nlfooter" footer-height="10%" padding="0.2in 0.2in 0.2in 0.2in" size="A5-landscape">

<table border='0' cellpadding='0' cellspacing='0' tyle='border-collapse:collapse;table-layout:fixed;width:570pt'>

 <tr style='height:20pt'>
  <td class='xl6318161' align='center' style='height:20pt;width:80pt;font-weight:700'>รหัสสินค้า</td>
  <td colspan='4' class='xl6318161' align='center' style='width:200pt;font-weight:700'>รายการ</td>
  <td class='xl6318161' align='center' style='width:50pt;font-weight:700'>จำนวน</td>
  <td class='xl6318161' align='center' style='width:50pt;font-weight:700'>หน่วย</td>
  <td class='xl6318161' align='center' style='width:55pt;font-weight:700'>ตัดสต๊อก</td>
  <td class='xl6318161' align='center' style='width:60pt;font-weight:700'>ราคา/หน่วย</td>
  <td class='xl6318161' align='center' style='width:75pt;font-weight:700'>จำนวนเงิน(บาท)</td>
 </tr>
 <#list data.itemwo as itemwo>
  <tr style='height:20pt'>
  <td class='xl6318161' align='center' style='height:20pt;font-size:9pt;vertical-align:top'>${itemwo.itemwo}</td>
  <td colspan='4' align='left' class='xl6518161' style='font-size:9pt;vertical-align:top'>${itemwo.itemnamewo}</td>
  <td class='xl6618161' align='center' style='vertical-align:top'>${itemwo.qtywo}</td>
  <td class='xl6318161' align='center' style='font-size:9pt;vertical-align:top'>${itemwo.unitwo}</td>
  <td class='xl6618161' align='center' style='vertical-align:top'>Cut stock</td>
  <td class='xl6618161' align='right' style='vertical-align:top'></td>
  <td class='xl6618161' align='right' style='vertical-align:top'></td>
 </tr>
  </#list>

 <#assign x =1/>
<#list data.item as item>
 <tr style='height:20pt'>
  <td class='xl6318161' align='center' style='font-size:9pt;height:20pt;vertical-align:top'>${item.itemnum}</td>
  <td colspan='4' align='left' class='xl6518161' style='font-size:9pt;vertical-align:top'>${item.itemname}</td>
  <td class='xl6618161' align='center' style='vertical-align:top'>${item.qty}</td>
  <td class='xl6318161' align='center' style='font-size:9pt;vertical-align:top'>${item.unit}</td>
  <td class='xl6618161' align='center' style='vertical-align:top'>Cut stock</td>
  <td class='xl6618161' align='right' style='vertical-align:top'></td>
  <td class='xl6618161' align='right' style='vertical-align:top'></td>
 </tr>
 <#assign x =x+1/>
 </#list>

 <#if x lte 9>
<#assign y=9-x>
<#else>
<#assign y=x%9>
</#if>
<#list 1..y as i>

 <tr style='height:20pt'>
  <td class='xl6318161'></td>
  <td colspan='4' class='xl6318161'></td>
  <td class='xl6318161'></td>
  <td class='xl6318161'></td>
  <td class='xl6318161'></td>
  <td class='xl6318161'></td>
  <td class='xl6318161'></td>
 </tr>
  <#assign x =x+1/>
 </#list>

</table>

