				<td class='xl71' style='border-left:none'>BU</td>
				<td colspan='4' class='xl73' style='border-right:.5pt solid black;border-left:  none;'>Description</td>
				<td class='xl71' style='border-left:none'>Debit</td>
				<td class='xl71' style='border-left:none'>Credit</td>
			</tr>
			<#assign x =1/>
			<#list data.glLines as glLine>
			<tr style='height:16.0pt'>
				<td align='left' style='height:16.0pt' vertical-align='middle'> &#160; ${glLine.item}</td>
				<td class='xl63' vertical-align='middle'>${glLine.accountnumber}</td>
				<td class='xl63' vertical-align='middle'>${glLine.deptCode?keep_before(" ")}</td>
				<td class='xl63' vertical-align='middle'>${glLine.classCode?keep_before(" ")}</td>
				<td colspan='4' class='xl68' style='border-top:none' vertical-align='middle'>${glLine.account}</td>
				<td align='right' vertical-align='middle'>${glLine.debitamount}&#160;</td>
				<td align='right' vertical-align='middle'>${glLine.creditamount}&#160;</td>
			</tr>
			</#list>
			<#if x lte 9>
			<#assign y=9-x>
			<#else>
			<#assign y=x%9> </#if>
			<#list 1..y as i>
			<tr style='height:16.0pt'>
				<td align='left' style='height:16.0pt'> </td>
